        BEGIN
            V_FIELD := TYP_FIELD();
            UTL_FILE.GET_LINE(V_FILE, V_LINE);
            --
            FOR I IN 1..132 LOOP
                V_FIELD.EXTEND;
                V_FIELD(I).LINE := INSTR(V_LINE, '|', V_LAST+1, 1);
                V_LAST := V_FIELD(I).LINE;
            END LOOP;

