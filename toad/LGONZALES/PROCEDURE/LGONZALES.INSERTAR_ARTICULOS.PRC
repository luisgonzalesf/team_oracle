        LINE NUMBER,
        V_ART LGONZALES.ARTICULOS_COPIA%ROWTYPE
    );
    TYPE TYP_FIELD IS
        TABLE OF TYP_REC;
    V_FIELD TYP_FIELD;
    V_LAST  NUMBER := 0;
    V_FILE      UTL_FILE.FILE_TYPE;


    V_LINE  VARCHAR2(1500);
