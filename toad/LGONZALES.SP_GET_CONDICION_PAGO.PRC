CREATE OR REPLACE PROCEDURE LGONZALES.SP_GET_CONDICION_PAGO(CUR_DATA OUT SYS_REFCURSOR)
IS
BEGIN
    OPEN CUR_DATA FOR 
        SELECT LLAVE CODIGO, DESC1 DESCRIPCION, DESC2, DESCY CANAL, FLAG10 TIPO-- 1Credito, 2 contado, 3 adelantado
            ,ESTADO
        FROM SYSADM.TABLAS 
        WHERE CATEGORIA = '008'
    ;
    COMMIT;
    DBMS_OUTPUT.PUT_LINE ('OK');
EXCEPTION
    WHEN OTHERS
    THEN DBMS_OUTPUT.PUT_LINE ('OCURRIO UN ERROR');
END;
/
